{"ast":null,"code":"import axios from 'axios';\nimport Global from '../Global';\nimport { computed } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  name: 'ProductPage',\n  setup() {\n    const url = Global.url;\n    const store = useStore();\n    const products = computed(() => store.state.products);\n    const searchQuery = computed(() => store.getters.searchQuery);\n    const filteredProducts = computed(() => {\n      if (!searchQuery.value) {\n        return products.value;\n      }\n      return products.value.filter(product => product.name.toLowerCase().includes(searchQuery.value.toLowerCase()));\n    });\n    return {\n      products,\n      searchQuery,\n      filteredProducts\n    };\n  },\n  methods: {\n    getPersonas() {\n      axios.get(this.url + '/Product/products').then(res => {\n        if (res.data.status == 'success') {\n          this.$store.commit('updateProducts', res.data.data);\n        }\n      }).catch(err => console.log(err));\n    }\n  },\n  created() {\n    this.getPersonas();\n  }\n};","map":{"version":3,"names":["axios","Global","computed","useStore","name","setup","url","store","products","state","searchQuery","getters","filteredProducts","value","filter","product","toLowerCase","includes","methods","getPersonas","get","then","res","data","status","$store","commit","catch","err","console","log","created"],"sources":["D:\\VS Code\\WebProject\\web-project\\src\\views\\ProductPage.vue"],"sourcesContent":["<template>\r\n    <h1>Productos</h1>\r\n    <div class=\"grid-container\">\r\n        <div class=\"card\" v-for=\"product in filteredProducts\" :key=\"product._id\">\r\n            <h2>{{ product.name }}</h2>\r\n            <p>Precio: ${{ product.price }}</p>\r\n            <button>Order</button>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<style scoped src=\"@/assets/css/ProductStyle.css\"></style>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport Global from '../Global'\r\nimport {computed} from 'vue';\r\nimport {useStore} from 'vuex';\r\n\r\nexport default {\r\n    name: 'ProductPage',\r\n    setup() {\r\n        const url = Global.url;\r\n        const store = useStore();\r\n\r\n        const products = computed(() => store.state.products);\r\n        const searchQuery = computed(() => store.getters.searchQuery);\r\n\r\n        const filteredProducts = computed(() => {\r\n          if (!searchQuery.value) {\r\n            return products.value;\r\n          }\r\n          return products.value.filter((product) =>\r\n            product.name.toLowerCase().includes(searchQuery.value.toLowerCase())\r\n          );\r\n        });\r\n\r\n        return {\r\n          products,\r\n          searchQuery,\r\n          filteredProducts,\r\n        }\r\n    },\r\n    methods: {\r\n        getPersonas(){\r\n            axios.get(this.url+'/Product/products')\r\n                .then(res => {\r\n                    if(res.data.status == 'success'){\r\n                        this.$store.commit('updateProducts', res.data.data)\r\n                    }\r\n                })\r\n                .catch(err => console.log(err))\r\n        }\r\n    },\r\n    created() {\r\n        this.getPersonas();\r\n    }\r\n}\r\n</script>\r\n"],"mappings":"AAcA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,WAAU;AAC7B,SAAQC,QAAQ,QAAO,KAAK;AAC5B,SAAQC,QAAQ,QAAO,MAAM;AAE7B,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,KAAKA,CAAA,EAAG;IACJ,MAAMC,GAAE,GAAIL,MAAM,CAACK,GAAG;IACtB,MAAMC,KAAI,GAAIJ,QAAQ,CAAC,CAAC;IAExB,MAAMK,QAAO,GAAIN,QAAQ,CAAC,MAAMK,KAAK,CAACE,KAAK,CAACD,QAAQ,CAAC;IACrD,MAAME,WAAU,GAAIR,QAAQ,CAAC,MAAMK,KAAK,CAACI,OAAO,CAACD,WAAW,CAAC;IAE7D,MAAME,gBAAe,GAAIV,QAAQ,CAAC,MAAM;MACtC,IAAI,CAACQ,WAAW,CAACG,KAAK,EAAE;QACtB,OAAOL,QAAQ,CAACK,KAAK;MACvB;MACA,OAAOL,QAAQ,CAACK,KAAK,CAACC,MAAM,CAAEC,OAAO,IACnCA,OAAO,CAACX,IAAI,CAACY,WAAW,CAAC,CAAC,CAACC,QAAQ,CAACP,WAAW,CAACG,KAAK,CAACG,WAAW,CAAC,CAAC,CACrE,CAAC;IACH,CAAC,CAAC;IAEF,OAAO;MACLR,QAAQ;MACRE,WAAW;MACXE;IACF;EACJ,CAAC;EACDM,OAAO,EAAE;IACLC,WAAWA,CA