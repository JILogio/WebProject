{"ast":null,"code":"import axios from 'axios';\nimport Global from '../Global';\nconst apiClient = axios.create({\n  withCredentials: true\n});\nexport default {\n  name: 'AddProduct',\n  data() {\n    return {\n      url: Global.url,\n      name: '',\n      type: '',\n      price: ''\n    };\n  },\n  methods: {\n    async handleSubmit() {\n      const product = {\n        name: this.name,\n        type: this.type,\n        price: this.price\n      };\n      try {\n        const response = await apiClient.post(this.url + 'Product/save', product);\n        if (response.data.status != 'error') {\n          alert('Producto agregado con éxito');\n          this.name = '';\n          this.type = '';\n          this.price = '';\n        } else {\n          console.log(response.data);\n        }\n      } catch (error) {\n        console.log(error);\n        alert(error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","Global","apiClient","create","withCredentials","name","data","url","type","price","methods","handleSubmit","product","response","post","status","alert","console","log","error"],"sources":["D:\\VS Code\\WebProject\\web-project\\src\\views\\AddProduct.vue"],"sourcesContent":["<template>\r\n    <h1>Agregar Producto</h1>\r\n    <div class=\"center\">\r\n       <form @submit.prevent=\"handleSubmit\">\r\n         <label for=\"name\">Nombre:</label>\r\n         <input type=\"text\" id=\"name\" v-model=\"name\" required />\r\n         \r\n         <label for=\"type\">Tipo:</label>\r\n         <input type=\"text\" id=\"type\" v-model=\"type\" required />\r\n         \r\n         <label for=\"price\">Precio:</label>\r\n         <input type=\"number\" id=\"price\" v-model=\"price\" required />\r\n         \r\n         <button type=\"submit\">Enviar</button>\r\n       </form>\r\n    </div>\r\n</template>\r\n\r\n<style scoped src=\"@/assets/css/AddProductStyle.css\"></style>\r\n   \r\n<script>\r\nimport axios from 'axios';\r\nimport Global from '../Global'\r\n\r\nconst apiClient = axios.create({\r\n  withCredentials: true,\r\n});\r\n   \r\n   export default {\r\n    name: 'AddProduct',\r\n    data() {\r\n       return {\r\n         url: Global.url,\r\n         name: '',\r\n         type: '',\r\n         price: '',\r\n       };\r\n    },\r\n    methods: {\r\n        async handleSubmit() {\r\n         const product = {\r\n           name: this.name,\r\n           type: this.type,\r\n           price: this.price\r\n        };\r\n         \r\n         try {\r\n           const response = await apiClient.post(this.url+'Product/save', product);\r\n           if(response.data.status != 'error'){\r\n            alert('Producto agregado con éxito');\r\n            this.name = '';\r\n            this.type = '';\r\n            this.price = '';\r\n           } else{\r\n            console.log(response.data);\r\n           }\r\n         } catch (error) {\r\n           console.log(error);\r\n           alert(error);\r\n         }\r\n       }\r\n    }\r\n   };\r\n</script>"],"mappings":"AAqBA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,WAAU;AAE7B,MAAMC,SAAQ,GAAIF,KAAK,CAACG,MAAM,CAAC;EAC7BC,eAAe,EAAE;AACnB,CAAC,CAAC;AAEC,eAAe;EACdC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACJ,OAAO;MACLC,GAAG,EAAEN,MAAM,CAACM,GAAG;MACfF,IAAI,EAAE,EAAE;MACRG,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE;IACT,CAAC;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,YAAYA,CAAA,EAAG;MACpB,MAAMC,OAAM,GAAI;QACdP,IAAI,EAAE,IAAI,CAACA,IAAI;QACfG,IAAI,EAAE,IAAI,CAACA,IAAI;QACfC,KAAK,EAAE,IAAI,CAACA;MACf,CAAC;MAEA,IAAI;QACF,MAAMI,QAAO,GAAI,MAAMX,SAAS,CAACY,IAAI,CAAC,IAAI,CAACP,GAAG,GAAC,cAAc,EAAEK,OAAO,CAAC;QACvE,IAAGC,QAAQ,CAACP,IAAI,CAACS,MAAK,IAAK,OAAO,EAAC;UAClCC,KAAK,CAAC,6BAA6B,CAAC;UACpC,IAAI,CAACX,IAAG,GAAI,EAAE;UACd,IAAI,CAACG,IAAG,GAAI,EAAE;UACd,IAAI,CAACC,KAAI,GAAI,EAAE;QAChB,OAAM;UACLQ,OAAO,CAACC,GAAG,CAACL,QAAQ,CAACP,IAAI,CAAC;QAC3B;MACF,EAAE,OAAOa,KAAK,EAAE;QACdF,OAAO,CAACC,GAAG,CAACC,KAAK,CAAC;QAClBH,KAAK,CAACG,KAAK,CAAC;MACd;IACF;EACH;AACD,CAAC"},"metadata":{},"sourceType":"mo