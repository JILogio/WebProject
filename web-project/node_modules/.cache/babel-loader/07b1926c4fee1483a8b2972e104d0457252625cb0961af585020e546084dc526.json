plate>\r\n    \r\n<script>\r\nimport axios from 'axios';\r\nimport Global from '@/Global';\r\n    \r\nconst apiClient = axios.create({\r\n    withCredentials: true,\r\n});\r\n    \r\nexport default {\r\n    name: 'RealizarPedido',\r\n    props: {ordersCopy: {type: Array}},\r\n    data() {\r\n        return {\r\n            url: Global.url,\r\n            location: {\r\n                street: '',\r\n                district: '',\r\n                postal: '',\r\n            },\r\n          };\r\n        },\r\n    created(){\r\n        console.log(this.orders)\r\n    },\r\n    mounted() {\r\n        this.ordersCopy = [...this.$route.params.orders];\r\n    },\r\n    methods: {\r\n    async submitForm() {\r\n        try {\r\n            // Aquí va el código para enviar los datos del formulario al servidor\r\n            await apiClient.post(this.url+'Location/save', this.location);\r\n\r\n            // Si todo sale bien, redirige al usuario a la página deseada\r\n            //this.$router.push({name: 'OrdersPage'});\r\n        } catch (error) {\r\n            console.error('Error submitting form:', error);\r\n        }\r\n      }\r\n    }\r\n}\r\n</script>"],"mappings":"AAkBA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,UAAU;AAE7B,MAAMC,SAAQ,GAAIF,KAAK,CAACG,MAAM,CAAC;EAC3BC,eAAe,EAAE;AACrB,CAAC,CAAC;AAEF,eAAe;EACXC,IAAI,EAAE,gBAAgB;EACtBC,KAAK,EAAE;IAACC,UAAU,EAAE;MAACC,IAAI,EAAEC;IAAK;EAAC,CAAC;EAClCC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,GAAG,EAAEV,MAAM,CAACU,GAAG;MACfC,QAAQ,EAAE;QACNC,MAAM,EAAE,EAAE;QACVC,QAAQ,EAAE,EAAE;QACZC,MAAM,EAAE;MACZ;IACF,CAAC;EACH,CAAC;EACLC,OAAOA,CAAA,EAAE;IACLC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACC,MAAM;EAC3B,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACb,UAAS,GAAI,CAAC,GAAG,IAAI,CAACc,MAAM,CAACC,MAAM,CAACH,MAAM,CAAC;EACpD,CAAC;EACDI,OAAO,EAAE;IACT,MAAMC,UAAUA,CAAA,EAAG;MACf,IAAI;QACA;QACA,MAAMtB,SAAS,CAACuB,IAAI,CAAC,IAAI,CAACd,GAAG,GAAC,eAAe,EAAE,IAAI,CAACC,QAAQ,CAAC;;QAE7D;QACA;MACJ,EAAE,OAAOc,KAAK,EAAE;QACZT,OAAO,CAACS,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAA