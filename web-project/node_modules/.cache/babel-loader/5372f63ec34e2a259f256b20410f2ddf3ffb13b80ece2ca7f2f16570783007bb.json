       }\r\n      },\r\n      async deleteOrder(id) {\r\n        try {\r\n          await apiClient.delete(this.url+'Order/delete/'+id);\r\n          this.orders = this.orders.filter(order => order._id !== id);\r\n        } catch (error) {\r\n          console.error('Error deleting order:', error);\r\n        }\r\n      },\r\n      isToday(dateString) {\r\n          const orderDate = new Date(dateString);\r\n          const today = new Date();\r\n          return orderDate.toDateString() === today.toDateString();\r\n      },\r\n      computed: {\r\n        filteredOrders() {\r\n            return this.orders.filter(order => this.isToday(order.createdAt));\r\n        }\r\n      },\r\n      pedir() {\r\n        const orderIds = this.orders.map(order => order._id);\r\n        this.$router.push({name: 'RealizarPedido', params: { orders: orderIds }});\r\n      },\r\n      saveLocation(location) {\r\n        console.log('Location saved:', location);\r\n      },\r\n    }\r\n}\r\n</script>\r\n"],"mappings":";AA+CA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,UAAU;AAC7B,OAAOC,iBAAgB,MAAO,yBAAyB;AAEvD,MAAMC,SAAQ,GAAIH,KAAK,CAACI,MAAM,CAAC;EAC7BC,eAAe,EAAE;AACnB,CAAC,CAAC;AAEF,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACRL;EACJ,CAAC;EACDM,IAAIA,CAAA,EAAG;IACJ,OAAO;MACLC,GAAG,EAAER,MAAM,CAACQ,GAAG;MACfC,MAAM,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACP,IAAI,CAACC,WAAW,CAAC,CAAC;EACrB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,WAAWA,CAAA,EAAG;MAClB,IAAI;QACA,MAAME,QAAO,GAAI,MAAMX,SAAS,CAACY,GAAG,CAAC,IAAI,CAACN,GAAG,GAAC,cAAc,CAAC;QAC7D,IAAI,CAACC,MAAK,GAAII,QAAQ,CAACN,IAAI,CAACE,MAAM;QAElC,KAAK,IAAIM,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAI,IAAI,CAACN,MAAM,CAACO,MAAM,EAAED,CAAC,EAAE,EAAE;UAC3C,MAAME,eAAc,GAAI,MAAMf,SAAS,CAACY,GAAG,CAAC,IAAI,CAACN,GAAG,GAAC,yBAAyB,GAAC,IAAI,CAACC,MAAM,CAACM,CAAC,CAAC,CAACG,SAAS,CAAC;UACxG,IAAI,CAACT,MAAM,CAACM,CAAC,CAAC,CAACI,WAAU,GAAIF,eAAe,CAACV,IAAI,CAACa,OAAO,CAAC,CAAC,CAAC,CAACf,IAAI;QACnE;MACJ,EAAE,OAAOgB,KAAK,EAAE;QACZC,OAAO,CAACD,KAAK,CAAC,0BAA0B,EAAE