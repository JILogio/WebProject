n    },\r\n    methods: {\r\n      async fetchOrders() {\r\n        try {\r\n          const response = await apiClient.get(this.url+'Order/orders');\r\n          this.orders = response.data.orders.map(order => ({\r\n            ...order,\r\n            idProduct: this.products.find(product => product._id === order.idProduct).name,\r\n          }));\r\n        } catch (error) {\r\n          console.error('Error fetching products:', error);\r\n        }\r\n      },\r\n      async fetchProducts() {\r\n        try {\r\n          const response = await apiClient.get(this.url+'Product/products');\r\n          this.products = response.data.products;\r\n        } catch (error) {\r\n          console.error('Error fetching products:', error);\r\n        }\r\n      }\r\n    }\r\n}\r\n</script>\r\n"],"mappings":"AA2CA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,UAAU;AAE7B,MAAMC,SAAQ,GAAIF,KAAK,CAACG,MAAM,CAAC;EAC9BC,eAAe,EAAE;AAClB,CAAC,CAAC;AAEF,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACJ,OAAO;MACLC,GAAG,EAAEN,MAAM,CAACM,GAAG;MACfC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACP,IAAI,CAACC,WAAW,CAAC,CAAC;IAClB,IAAI,CAACC,aAAa,CAAC,CAAC;EACvB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMF,WAAWA,CAAA,EAAG;MAClB,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMZ,SAAS,CAACa,GAAG,CAAC,IAAI,CAACR,GAAG,GAAC,cAAc,CAAC;QAC7D,IAAI,CAACC,MAAK,GAAIM,QAAQ,CAACR,IAAI,CAACE,MAAM,CAACQ,GAAG,CAACC,KAAI,KAAM;UAC/C,GAAGA,KAAK;UACRC,SAAS,EAAE,IAAI,CAACT,QAAQ,CAACU,IAAI,CAACC,OAAM,IAAKA,OAAO,CAACC,GAAE,KAAMJ,KAAK,CAACC,SAAS,CAAC,CAACb;QAC5E,CAAC,CAAC,CAAC;MACL,EAAE,OAAOiB,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,0BAA0B,EAAEA,KAAK,CAAC;MAClD;IACF,CAAC;IACD,MAAMV,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMZ,SAAS,CAACa,GAAG,CAAC,IAAI,CAACR,GAAG,GAAC,kBAAkB,CAAC;QACjE,IAAI,CAACE,QAAO,GAAIK,QAAQ,CAACR,IAAI,CAACG,QAAQ;MACxC,EAAE,OAAOa,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,0BAA0B,EAAEA,KAAK,CAAC;MAClD;IACF;EACF;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}