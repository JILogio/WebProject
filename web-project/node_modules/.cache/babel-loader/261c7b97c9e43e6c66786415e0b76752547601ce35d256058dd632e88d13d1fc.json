{"ast":null,"code":"import axios from 'axios';\nimport Global from '@/Global';\nconst apiClient = axios.create({\n  withCredentials: true\n});\nexport default {\n  name: \"ReportPage\",\n  data() {\n    return {\n      url: Global.url,\n      invoices: []\n    };\n  },\n  methods: {\n    async getAllInvoices() {\n      try {\n        const response = await apiClient.get(this.url + 'Report/InvoicesWithDistrict');\n        this.invoices = response.data;\n      } catch (error) {\n        console.error('Error fetching products:', error);\n      }\n    },\n    async FindProductsByDistrict() {\n      try {\n        const response = await apiClient.get(this.url + 'Report/InvoicesLocation/');\n        this.invoices = response.data;\n      } catch (error) {\n        console.error('Error', error);\n      }\n    },\n    formatDate(date) {\n      let formattedDate = new Date(date);\n      let options = {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit'\n      };\n      return formattedDate.toLocaleDateString('es-ES', options);\n    }\n  }\n};","map":{"version":3,"names":["axios","Global","apiClient","create","withCredentials","name","data","url","invoices","methods","getAllInvoices","response","get","error","console","FindProductsByDistrict","formatDate","date","formattedDate","Date","options","year","month","day","toLocaleDateString"],"sources":["D:\\VS Code\\WebProject\\web-project\\src\\views\\ReportPage.vue"],"sourcesContent":["<template>\r\n    <button @click=\"getAllInvoices\">Obtener facturas</button>\r\n    <button @click=\"FindProductsByDistrict\">Listar productos por distrito</button>\r\n\r\n    <!-- Invoices Template -->\r\n    <div v-if=\"invoices.length > 0\">\r\n        <h2>Facturas</h2>\r\n        <table>\r\n            <thead>\r\n                <tr>\r\n                    <th>Fecha</th>\r\n                    <th>Total</th>\r\n                    <th>Distrito</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody>\r\n                <tr v-for=\"invoice in invoices\" :key=\"invoice.id\">\r\n                    <td>{{ invoice.date }}</td>\r\n                    <td>{{ invoice.totalValue }}</td>\r\n                    <td>{{ formatDate(invoice.district) }}</td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n\r\n<style scoped src=\"@/assets/css/AdminProductStyle.css\"></style>\r\n \r\n<script>\r\nimport axios from 'axios';\r\nimport Global from '@/Global';\r\n\r\nconst apiClient = axios.create({\r\nwithCredentials: true,\r\n});\r\n \r\n export default {\r\n  name: \"ReportPage\",\r\n  data() {\r\n     return {\r\n       url: Global.url,\r\n       invoices: [],\r\n     };\r\n  },\r\n  methods: {\r\n    async getAllInvoices() {\r\n        try {\r\n            const response = await apiClient.get(this.url+'Report/InvoicesWithDistrict');\r\n            this.invoices = response.data;\r\n        } catch (error) {\r\n            console.error('Error fetching products:', error);\r\n        }\r\n    },\r\n    async FindProductsByDistrict(){\r\n        try {\r\n            const response = await apiClient.get(this.url+'Report/InvoicesLocation/');\r\n            this.invoices = response.data;\r\n        } catch (error) {\r\n            console.error('Error', error);\r\n        }\r\n    },\r\n    formatDate(date) {\r\n        let formattedDate = new Date(date);\r\n        let options = { year: 'numeric', month: '2-digit', day: '2-digit' };\r\n        return formattedDate.toLocaleDateString('es-ES', options);\r\n    }\r\n  },\r\n\r\n };\r\n</script>\r\n"],"mappings":"AA6BA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,UAAU;AAE7B,MAAMC,SAAQ,GAAIF,KAAK,CAACG,MAAM,CAAC;EAC/BC,eAAe,EAAE;AACjB,CAAC,CAAC;AAED,eAAe;EACdC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACJ,OAAO;MACLC,GAAG,EAAEN,MAAM,CAACM,GAAG;MACfC,QAAQ,EAAE;IACZ,CAAC;EACJ,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,cAAcA,CAAA,EAAG;MACnB,IAAI;QACA,MAAMC,QAAO,GAAI,MAAMT,SAAS,CAACU,GAAG,CAAC,IAAI,CAACL,