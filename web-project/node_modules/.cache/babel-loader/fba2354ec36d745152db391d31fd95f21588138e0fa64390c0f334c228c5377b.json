mit() {\r\n         const product = {\r\n           name: this.name,\r\n           type: this.type,\r\n           price: this.price\r\n         };\r\n         \r\n         try {\r\n           await axios.post(this.url+'/Product/save', product);\r\n           alert('Producto agregado con éxito');\r\n           this.name = '';\r\n           this.type = '';\r\n           this.price = '';\r\n         } catch (error) {\r\n           console.error(error);\r\n           alert('Error al agregar el producto');\r\n         }\r\n       }\r\n    }\r\n   };\r\n</script>"],"mappings":"AAqBG,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,WAAU;AAE7B,eAAe;EACdC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACJ,OAAO;MACLC,GAAG,EAAEH,MAAM,CAACG,GAAG;MACfF,IAAI,EAAE,EAAE;MACRG,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE;IACT,CAAC;EACJ,CAAC;EACDC,OAAO,EAAE;IACN,MAAMC,YAAYA,CAAA,EAAG;MACnB,MAAMC,OAAM,GAAI;QACdP,IAAI,EAAE,IAAI,CAACA,IAAI;QACfG,IAAI,EAAE,IAAI,CAACA,IAAI;QACfC,KAAK,EAAE,IAAI,CAACA;MACd,CAAC;MAED,IAAI;QACF,MAAMN,KAAK,CAACU,IAAI,CAAC,IAAI,CAACN,GAAG,GAAC,eAAe,EAAEK,OAAO,CAAC;QACnDE,KAAK,CAAC,6BAA6B,CAAC;QACpC,IAAI,CAACT,IAAG,GAAI,EAAE;QACd,IAAI,CAACG,IAAG,GAAI,EAAE;QACd,IAAI,CAACC,KAAI,GAAI,EAAE;MACjB,EAAE,OAAOM,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;QACpBD,KAAK,CAAC,8BAA8B,CAAC;MACvC;IACF;EACH;AACD,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}