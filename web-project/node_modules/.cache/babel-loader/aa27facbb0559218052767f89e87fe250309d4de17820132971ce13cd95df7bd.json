n        }\r\n    },\r\n    methods: {\r\n        getProductos(){\r\n            apiClient.get(this.url+'Product/products')\r\n                .then(res => {\r\n                    if(res.data.status == 'success'){\r\n                        this.products = res.data.products;\r\n                    }\r\n                })\r\n                .catch (err => {\r\n                    console.log(err)\r\n                })\r\n            },\r\n        async orderProduct(id, event) {\r\n            event.preventDefault();\r\n            console.log(amount)\r\n        }\r\n    }\r\n}\r\n</script>"],"mappings":"AAeA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,WAAU;AAE7B,MAAMC,SAAQ,GAAIF,KAAK,CAACG,MAAM,CAAC;EAC7BC,eAAe,EAAE;AACnB,CAAC,CAAC;AAEF,eAAe;EACXC,IAAI,EAAE,aAAa;EAEnBC,OAAOA,CAAA,EAAE;IACL,IAAI,CAACC,YAAY,CAAC,CAAC;EACvB,CAAC;EACDC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,GAAG,EAAER,MAAM,CAACQ,GAAG;MACfC,QAAQ,EAAE,CAAC,CAAC;MACZC,MAAM,EAAE;IACZ;EACJ,CAAC;EACDC,OAAO,EAAE;IACLL,YAAYA,CAAA,EAAE;MACVL,SAAS,CAACW,GAAG,CAAC,IAAI,CAACJ,GAAG,GAAC,kBAAkB,EACpCK,IAAI,CAACC,GAAE,IAAK;QACT,IAAGA,GAAG,CAACP,IAAI,CAACQ,MAAK,IAAK,SAAS,EAAC;UAC5B,IAAI,CAACN,QAAO,GAAIK,GAAG,CAACP,IAAI,CAACE,QAAQ;QACrC;MACJ,CAAC,EACAO,KAAI,CAAGC,GAAE,IAAK;QACXC,OAAO,CAACC,GAAG,CAACF,GAAG;MACnB,CAAC;IACL,CAAC;IACL,MAAMG,YAAYA,CAACC,EAAE,EAAEC,KAAK,EAAE;MAC1BA,KAAK,CAACC,cAAc,CAAC,CAAC;MACtBL,OAAO,CAACC,GAAG,CAACT,MAAM;IACtB;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}